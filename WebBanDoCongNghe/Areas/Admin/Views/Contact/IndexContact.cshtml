
@{
    ViewBag.Title = "IndexContact";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}


@section naviheader{
    <!-- Left navbar links -->
    <ul class="navbar-nav">
        <li class="nav-item">
            <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
        </li>
        <li class="nav-item d-none d-sm-inline-block">
            <a href="/Admin/Home/Index" class="nav-link">Trang chủ</a>
        </li>
        <li class="nav-item d-none d-sm-inline-block">
            <a href="#" class="nav-link">Liên hệ của thành viên</a>
        </li>
    </ul>
}
<!-- Content Header (Page header) -->
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>Liên hệ của khách hàng</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="/Admin/Home/Index">Home</a></li>
                    <li class="breadcrumb-item active">Liên hệ của khách hàng</li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <!--.card-header -->
                <div class="card-header p-2">
                    <ul class="nav nav-pills">
                        <li class="nav-item"><a class="nav-link active" href="#Contact" data-toggle="tab">Liên hệ mới</a></li>
                        <li class="nav-item"><a class="nav-link" href="#CompleteContact" data-toggle="tab">Liên hệ đã xử lý </a></li>

                    </ul>
                </div>
                <!--/.card-header -->
                <!--.card-body -->
                <div class="card-body">
                    <!--.tab-content -->
                    <div class="tab-content">
                        <div class="tab-pane active" id="Contact">
                            @Html.Action("Contact", "Contact")
                        </div>
                        <div class="tab-pane" id="CompleteContact">
                            @Html.Action("CompleteContact", "Contact")
                        </div>

                    </div>
                </div>
                <!--/.tab-content -->
            </div>
            <!--/.card-body -->
        </div>
    </div>
</section>






